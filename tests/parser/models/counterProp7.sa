module modone


    x : clock;
    y : clock;
    z : clock;
    t : [0..3]  init 0;
    q : [0..3]  init 0;


    /* WARNING: */
    [d?] t==0 -> (t'=1);
    [d?] t==0 -> (t'=2);
    /* WARNING: */
    [e?] t==0 -> (t'=1) & (x'=normal(0,2)) 
                        & (y'=exponential(1)) 
                        & (z'=exponential(2));
    [e?] t==0 -> (t'=1);
    /* WARNING: */
    [h?] q==3 -> (t'=q);
    [h?] t==1 -> (t'=2);
    /* NO WARNING: */
    [a?] true -> (t'=0);
    [a?] t==0 -> ;
    /* NO WARNING: */
    [b?] t==2 -> (t'=2);
    [b?] true -> (t'=2);
    /* NO WARNING: */
    [c?] t==2 -> (t'=2);
    [c?] t==1 -> (t'=9);
    /* NO WARNING: */
    [g?] t==2 -> (t'=2);
    [g?] t==1 -> (t'=9);
    /* NO WARNING: */
    [f?] q==2 -> (t'=q);
    [f?] t==1 -> (t'=2);

endmodule
