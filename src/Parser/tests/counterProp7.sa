MODULE modone


VARS

    clock       x = Normal(2,-2);
    clock       y = Normal(2,-2);
    clock       z = Normal(2,-2);
    int [0..3]  t = 0;
    int [0..3]  q = 0;

TRANS

    /* WARNING: */
    [d?] t==0 -> t=1;
    [d?] t==0 -> t=2;
    /* WARNING: */
    [e?] t==0 -> t=1 : x,y,z;
    [e?] t==0 -> t=1;
    /* WARNING: */
    [h?] q==3 -> t=q;
    [h?] t==1 -> t=2;
    /* NO WARNING: */
    [a?] true -> t=0;
    [a?] t==0 -> ;
    /* NO WARNING: */
    [b?] t==2 -> t=2;
    [b?] true -> t=2;
    /* NO WARNING: */
    [c?] t==2 -> t=2;
    [c?] t==1 -> t=9;
    /* NO WARNING: */
    [g?] t==2 -> t=2;
    [g?] t==1 -> t=9;
    /* NO WARNING: */
    [f?] q==2 -> t=q;
    [f?] t==1 -> t=2;

