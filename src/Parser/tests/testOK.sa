MODULE modone

LABELS
    a:input; 
    b:output;
    d:input;
    c:output;

CLOCKS
    clockone:  Normal(1,1);
    clocktwo:  Exponential(3);
    clockfour: Uniform(6,2);
    cc:        Normal(2,-2);

VARS
    bool v = false;
    int a = 2;
    bool c = false || true;
    bool b = false;
    int t = 0;

TRANS

    /* Some transition that * represents / something. */
    [b?] a > 0 && t > 0 && a + t + 4 == 9                       -> ;
    [a] t == 2 && true && false != (3.653  >= 3/9) : cc  -> a = 1, c = false ;
    [d?] c == 4 + 5 > a                             
                                                                -> a = 4+2, b = c||b  :  clockone, clockfour;
    [c!] (-(3+10/2)) < 0.2                         : cc         -> ;




MODULE modthree

CLOCKS
    clockthree: /* This clock has exponential delay. */
                Exponential(2);
    clockfour :Normal(-1,-2);

TRANS

    [] : clockthree -> ;
    [b] : clockfour -> : clockfour;



PROPERTY prop1 : 3/2 > modthree.clockthree +1 && false;
PROPERTY prop2 : false ;
